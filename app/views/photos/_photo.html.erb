<div class="media">
  <div class="media-left">
    <%= link_to image_tag(photo.image.url(:large), class: 'media-object'), photo.image.url, target: '_blank' %>
  </div>
  <div class="media-body">
    <h6>Photo description: </h6><h4 class="media-heading"><%= photo.name %></h4>
    <h6>Uploaded on: </h6><h4 class="media-heading"><%= photo.created_at %></h4>
  </div>
</div>
